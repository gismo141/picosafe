#!/bin/sh
set -e

if ! test -d /var/run/tmux; then
  install -g utmp -m 0775 -d /var/run/tmux
  [ `stat -c%a /usr/bin/tmux` -lt 4000 ] || chmod 0755 /var/run/tmux
fi

# Automatically added by dh_installinit
if [ -x "/etc/init.d/tmux-cleanup" ]; then
	update-rc.d tmux-cleanup start 70 S . >/dev/null || exit $?
fi
# End automatically added section

